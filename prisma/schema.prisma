generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Report {
  id          String   @id @default(uuid())
  title       String
  type        String?
  date        String?
  country     String?
  summary     String?
  site        String?
  aircraft    String?
  operator    String?
  fatalities  Int?
  injuries    Int?
  survivors   Int?
  origin      String?
  destination String?
  pdf_url     String?
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}